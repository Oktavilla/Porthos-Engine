<% @page_class = 'layout2' %>
<div id="sub_nav">
  <%= render :partial => 'admin/shared/settings_sub_nav' %>
</div>
<div id="workspace">
  <div class="header">
    <h1><%= t(:title, :scope => [:app, :views, :admin, :tags, :new]) %></h1>
  </div>
  <div id="content">
    <%= form_for :tag, :url => admin_tags_path do |f| %>
      <%= render 'admin/shared/error_messages', :object => f.object %>
      <%= f.text_field :name %>
      <div class="submit">
        <%= submit_tag t(:save) %> eller <%= link_to t(:cancel), previous_view_path(admin_tags_path) %>
      </div>
    <% end %>
  </div>
</div>
