<% @page_class = @renderer.layout_class -%>
<% @page_title = @renderer.title -%>
<% @page_id    = @renderer.page_id -%>
<div id="content">
  <h1><%= @renderer.title %></h2>
  <ul>
  <% @renderer.categories.each do |category| %>
    <li>
      <%= link_to category.name, field_set_category_path(@renderer.field_set, category) %>
    </li>
  <% end %>
  </ul>
</div>